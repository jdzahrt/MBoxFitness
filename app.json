{
  "expo": {
    "name": "MBoxFitness",
    "slug": "MBoxFitness",
    "version": "1.0.0",
    "orientation": "portrait",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "icon": "./app/assets/MBOX.jpeg",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./app/assets/MBOX.jpeg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "jsEngine": "hermes",
      "supportsTablet": true,
      "bundleIdentifier": "com.jesse.zahrt.MBoxFitness",
      "buildNumber": "1",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSPhotoLibraryUsageDescription": "This app needs access to your photo library to allow you to select and upload images for your fitness listings.",
        "NSCameraUsageDescription": "This app needs access to your camera to take photos for your fitness listings."
      }
    },
    "notification": {
      "icon": "./app/assets/MBOX.jpeg",
      "color": "#000000"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./app/assets/MBOX.jpeg",
          "color": "#ffffff"
        }
      ]
    ],
    "android": {
      "jsEngine": "hermes",
      "package": "com.jesse.zahrt.MBoxFitness",
      "permissions": [
        "android.permission.RECEIVE_BOOT_COMPLETED",
        "android.permission.VIBRATE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "83a39a02-22bf-4625-87cf-c4abf0daf512"
      }
    }
  }
}
